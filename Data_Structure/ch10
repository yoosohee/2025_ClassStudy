      "source": [
        "def binary_search(A, key, low, high):\n",
        "  if (low>high):\n",
        "    return -1\n",
        "\n",
        "  middle=(low+high) // 2\n",
        "\n",
        "  if(key==A[middle]): # 탐색 성공\n",
        "    return middle\n",
        "  elif(key<A[middle]):\n",
        "    return binary_search(A, key, low, middle-1)\n",
        "  else:\n",
        "    return binary_search(A, key, middle+1, high)"
      ],
      "metadata": {
        "id": "I4IRNm0ycd-8"
