      "source": [
        "def sequential_search(A, key, low, high):\n",
        "  for i in range(low, high+1):\n",
        "    if A[i] == key: # 탐색에 성공하면 키 값의 인덱스 반환\n",
        "      return i\n",
        "  return -1 # 탐색에 실패하면 -1 반환"
      ]
    },
