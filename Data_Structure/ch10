      "cell_type": "code",
      "source": [
        "def binary_search_iter(A,key,low,high):\n",
        "  while(low<=high):\n",
        "    middle=(low+high) // 2\n",
        "\n",
        "    if key==A[middle]:\n",
        "      return middle\n",
        "    elif(key>A[middle]):\n",
        "      low=middle+1\n",
        "    else:\n",
        "      high=middle-1\n",
        "    return -1"
      ],
      "metadata": {
        "id": "q-_TNvkEesx-"
      },
      "execution_count": 19,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# 문자열을 위한 해시 함수\n",
        "def hashFn(key, M): # M: 테이블 크기\n",
        "  sum=0\n",
        "  for c in key:\n",
        "    sum=sum+ord(c) # 문자의 아스키 코드값을 sum에 더함\n",
        "  return sum%M"
      ],
      "metadata": {
        "id": "OiGalD6IfJS9"
