      "source": [
        "M=13\n",
        "table=[None]*M\n",
        "def hashFn(key):\n",
        "  return key%M"
      ],
      "metadata": {
        "id": "rAFZlhWVqXAp"
      },
      "execution_count": 58,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def insert(key):\n",
        "  i=hashFn(key) # 해시 주소 계산\n",
        "  count=M\n",
        "  while count>0:\n",
        "    if table[i]==None:\n",
        "      break # 삽입 위치 발견\n",
        "    i=(i+1)%M # 다음 조사 위치\n",
        "    count-=1\n",
        "\n",
        "  if count>0: # 삽입할 곳이 있으면 삽입\n",
        "    table[i]=key"
      ],
      "metadata": {
        "id": "I4_MAmfBqj87"
      },
      "execution_count": 59,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def search(key):\n",
        "  i=hashFn(key)\n",
        "  count=M\n",
        "  while count>0:\n",
        "    if table[i]==None: # 탐색 실패. None 반환\n",
        "      return None\n",
        "    if table[i]==key: # 탐색 성공. 레코드 반환\n",
        "      return table[i]\n",
        "    i=(i+1)%M # 다음 조사 위치\n",
        "    count-=1\n",
        "\n",
        "  return None # 탐색 실패. None 반환"
      ],
      "metadata": {
        "id": "qY8sVHNFuhaq"
      },
      "execution_count": 60,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def delete(key):\n",
        "  i=hashFn(key)\n",
        "  count=M\n",
        "  while count>0:\n",
        "    if table[i]==key: # 삭제할 위치 발견: i\n",
        "      table[i]=-1 # -1을 저장. 사용되었다가 삭제된 경우임\n",
        "      return\n",
        "    if table[i]==None: # None은 테이블에 삭제할 레코드가 없다는 것을 의미\n",
        "      return\n",
        "    i=(i+1)%M # 다음 조사 위치\n",
        "    count-=1"
      ],
      "metadata": {
        "id": "Q4jZ0zqYu1uz"
      },
      "execution_count": 61,
      "outputs": []
    },
