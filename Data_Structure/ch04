        "def calc (n1,n2,op):\n",
        "  if op=='+':\n",
        "    result=n1+n2\n",
        "  elif op=='-':\n",
        "    result=n1-n2\n",
        "  elif op=='*':\n",
        "    result=n1*n2\n",
        "  else:\n",
        "    result=n1/n2\n",
        "  return result\n",
        "\n",
        "num1 = int(input(\"숫자 입력: \"))\n",
        "num2 = int(input(\"숫자 입력: \"))\n",
        "op = (input(\"계산을 입력하세요(+, -, *, /): \"))\n",
        "result = calc (num1, num2, op)\n",
        "print (result)"
