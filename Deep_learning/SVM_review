        "iris = datasets.load_iris()\n",
        "X_train, X_test, y_train, y_test = model_selection.train_test_split(iris.data, iris.target, test_size = 0.6, random_state=42)"
      ],
      "metadata": {
        "id": "_QsYJPi8mXtv"
